---
/* Importando layoutss */
import Layout from "../layouts/Layout.astro";

/* Importando componentes */
import IconSocial from "../components/IconSocial.astro";
import InfoContact from "../components/InfoContact.astro";
---

<Layout
	title="Página no encontrada"
	description="La página que está intentando ver no existe, o actualmente no está disponible."
>
	<main>
		<section>
			<article>
				<header>
					<h2>Error 404</h2>
				</header>

				<article>
					<p>
						La página que está intentando ver no existe, o
						actualmente no está disponible.
					</p>
				</article>
				<IconSocial />
				<InfoContact />
			</article>
		</section>
	</main>
</Layout>
